# Production Environment Configuration
NODE_ENV=production
EXPO_PUBLIC_ENV=production

# Firebase Production Configuration (set via EAS secrets in production)
EXPO_PUBLIC_FIREBASE_API_KEY_PROD=
EXPO_PUBLIC_FIREBASE_AUTH_DOMAIN_PROD=
EXPO_PUBLIC_FIREBASE_PROJECT_ID_PROD=
EXPO_PUBLIC_FIREBASE_STORAGE_BUCKET_PROD=
EXPO_PUBLIC_FIREBASE_MESSAGING_SENDER_ID_PROD=
EXPO_PUBLIC_FIREBASE_APP_ID_PROD=
EXPO_PUBLIC_FIREBASE_MEASUREMENT_ID_PROD=

# Production API URLs
EXPO_PUBLIC_API_URL=https://api.statlocker.app
EXPO_PUBLIC_AUTH_API_URL=https://auth.statlocker.app
EXPO_PUBLIC_STATS_API_URL=https://stats.statlocker.app
EXPO_PUBLIC_GOALS_API_URL=https://goals.statlocker.app
EXPO_PUBLIC_RECRUITING_API_URL=https://recruiting.statlocker.app

# Production Analytics
EXPO_PUBLIC_ANALYTICS_ENABLED=true
# Note: Actual keys should be set in EAS secrets, not in this file

# Production Features
EXPO_PUBLIC_FEATURE_ANALYTICS=true
EXPO_PUBLIC_FEATURE_HAPTICS=true
EXPO_PUBLIC_FEATURE_OFFLINE_SUPPORT=true
EXPO_PUBLIC_FEATURE_PUSH_NOTIFICATIONS=true
EXPO_PUBLIC_FEATURE_IN_APP_PURCHASES=true

# Production Debugging (disabled)
EXPO_PUBLIC_DEBUG_MODE=false
EXPO_PUBLIC_LOG_LEVEL=error
FLIPPER_ENABLED=false

# Production Performance
EXPO_PUBLIC_PERFORMANCE_MONITORING=true
EXPO_PUBLIC_BUNDLE_ANALYZER=false