{
  "name": "statlocker-mobile",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build:dev:ios": "eas build --platform ios --profile development",
    "build:dev:android": "eas build --platform android --profile development",
    "build:preview:ios": "eas build --platform ios --profile preview",
    "build:preview:android": "eas build --platform android --profile preview",
    "build:production": "eas build --platform all --profile production",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --watchAll=false",
    "test:unit": "jest --testPathPattern=\"__tests__\" --testPathIgnorePatterns=\"integration|accessibility\"",
    "test:integration": "jest --testPathPattern=\"integration\"",
    "test:ci": "jest --coverage --watchAll=false --ci --maxWorkers=2",
    "performance:check": "node scripts/performance-check.js",
    "performance:lint": "eslint . --config .eslintrc.performance.js --ext .ts,.tsx",
    "accessibility:check": "node scripts/accessibility-check.js",
    "accessibility:test": "npm test -- --testPathPattern=\"accessibility\"",
    "quality:check": "npm run type-check && npm run lint && npm run format:check && npm run accessibility:check",
    "ci:quality": "npm run type-check && npm run lint && npm run format:check",
    "ci:test": "npm run test:ci",
    "ci:security": "npm audit --audit-level=moderate",
    "replace-colors": "node scripts/replace-colors.js",
    "replace-colors:src": "node scripts/replace-colors.js \"src/**/*.{ts,tsx}\"",
    "replace-colors:app": "node scripts/replace-colors.js \"app/**/*.{ts,tsx}\"",
    "replace-assets": "node scripts/replace-assets.js"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-google-signin/google-signin": "^16.0.0",
    "expo": "~54.0.20",
    "expo-apple-authentication": "^8.0.7",
    "expo-auth-session": "^7.0.8",
    "expo-constants": "~18.0.10",
    "expo-crypto": "^15.0.7",
    "expo-device": "^8.0.9",
    "expo-haptics": "^15.0.7",
    "expo-linear-gradient": "~15.0.7",
    "expo-linking": "~8.0.8",
    "expo-network": "~8.0.7",
    "expo-router": "~6.0.13",
    "expo-secure-store": "^15.0.7",
    "expo-status-bar": "~3.0.8",
    "firebase": "^12.4.0",
    "lucide-react-native": "^0.548.0",
    "nativewind": "^4.2.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-reanimated": "^4.1.3",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-web": "^0.21.0",
    "react-native-worklets-core": "^1.6.2",
    "tailwindcss": "^3.4.0",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "^29.5.14",
    "@types/react": "~19.1.0",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "babel-plugin-module-resolver": "^5.0.2",
    "babel-preset-expo": "^54.0.6",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-native": "^5.0.0",
    "glob": "^11.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.2.0",
    "prettier": "^3.6.2",
    "react-native-worklets": "^0.5.1",
    "react-test-renderer": "^19.1.0",
    "typescript": "~5.9.2"
  },
  "private": true
}
